{
   "apiVersion": "extensions/v1beta1",
   "kind": "Deployment",
   "metadata": {
      "annotations": {
         "deployment.kubernetes.io/revision": "1"
      },
      "generation": 1,
      "labels": {
         "run": "grpc-server"
      },
      "name": "grpc-server",
      "namespace": "default"
   },
   "spec": {
      "replicas": 1,
      "selector": {
         "matchLabels": {
            "run": "grpc-server"
         }
      },
      "strategy": {
         "rollingUpdate": {
            "maxSurge": 1,
            "maxUnavailable": 1
         },
         "type": "RollingUpdate"
      },
      "template": {
         "metadata": {
            "creationTimestamp": null,
            "labels": {
               "run": "grpc-server"
            }
         },
         "spec": {
            "containers": [
               {
                  "image": "us.gcr.io/mchirico/gog:inst",
                  "imagePullPolicy": "Always",
                  "name": "grpc-server",
                  "command": [
                     "grpc"
                  ],
                  "ports": [
                     {
                        "containerPort": 8082,
                        "protocol": "TCP"
                     },
                     {
                        "containerPort": 8081,
                        "protocol": "TCP"
                     }
                  ],
                  "resources": {},
                  "terminationMessagePath": "/dev/termination-log",
                  "terminationMessagePolicy": "File"
               }
            ],
            "dnsPolicy": "ClusterFirst",
            "restartPolicy": "Always",
            "schedulerName": "default-scheduler",
            "securityContext": {},
            "terminationGracePeriodSeconds": 30
         }
      }
   }
}
